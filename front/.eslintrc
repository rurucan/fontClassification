{
	"root": true,
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": "airbnb",
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module"
	},
	"globals": {
		"window": true,
		"global": true,
		"document": true
	},
	"rules": {
		"import/no-extraneous-dependencies": [0],
		"import/prefer-default-export": [0],
		"react/prop-types": [0],
		"react/jsx-filename-extension": [0],
		"no-mixed-operators": [0],
		"spaced-comment": [0],
		"no-console": [0],
		"global-require": [0],
		"func-names": [0],
		"no-param-reassign": ["error", { "props": false }],
		"prefer-promise-reject-errors": [0],
		"no-multi-assign": [0],
		"object-curly-newline": ["error", { "multiline": true, "consistent": true }]
	},
	"settings": {
		"import/core-modules": ["electron"],
		"import/resolver": {
			"configurable": {
				"@render": "./render/"
			},
			"node": {
				"extensions": [".js", ".jsx", ".pro.js", ".dev.js"]
			}
		}
	}
}